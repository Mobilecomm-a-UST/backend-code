# Create DataFrame
df = pd.DataFrame(data, columns=["Node", "MO", "Attribute", "Value"])

# Step 1: Filter relevant rows
df = df[
    df["MO"].str.contains(r"^EUtranCell(FDD|TDD)=", na=False, regex=True) &
    df["Attribute"].str.contains(r"sectorCarrierRef", na=False, regex=True)
]

# Step 2: Clean Value column (extract number after '=')
df["Value"] = df["Value"].apply(lambda x: re.search(r"=(\d+)", str(x)).group(1) if re.search(r"=(\d+)", str(x)) else x)

# Step 3: Drop duplicate MO/Value pairs
df = df.drop_duplicates(subset=["MO", "Value"])

# Step 4: Group SectorCarrier values by MO
df_grouped = df.groupby("MO")["Value"].apply(lambda x: sorted(set(x))).reset_index()

# Step 5: Print final result
print("Grouped SectorCarrier values per MO:\n")
print(df_grouped.to_string(index=False))
